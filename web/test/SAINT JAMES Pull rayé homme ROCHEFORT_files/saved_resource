				$('#icone-panier').click(function () {
					$('#popup-panier').clearQueue();
					$('#popup-panier').stop();
					$('#popup-panier').toggle('blind', {}, 'fast' );
				});

				$('#icone-panier').mouseenter(function () {
					if ($('#popup-panier').is(':visible')) {
					} else {
						$('#popup-panier').clearQueue();
						$('#popup-panier').stop();
						$('#popup-panier').show('blind', {}, 'fast' );
					}
				});

				$('#icone-panier').mouseleave(function () {
					if ($('#popup-panier').is(':visible')) {
						$('#popup-panier').clearQueue();
						$('#popup-panier').stop();
						$('#popup-panier').hide('blind', {}, 'fast' );
					}
				});
