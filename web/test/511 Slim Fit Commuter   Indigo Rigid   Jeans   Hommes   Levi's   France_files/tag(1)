try { if (eval('bt_data(\x27use of cookies element\x27)')) {
serverURL.cf(21285);
}
} catch(e) { bt_handle_exception(e); }
try { if (eval('!bt_data(\x27secure pages only\x27)')) {
serverURL.cf(49095);
}
} catch(e) { bt_handle_exception(e); }
try { if (eval('bt_data(\x27secure pages only\x27)')) {
serverURL.cf(49096);
}
} catch(e) { bt_handle_exception(e); }
serverURL.appendData('pid parameter');
});
});
