var fc = {}; // footer email capture namespace
fc.validateEmailSignup = function() {
	
	fc.clearErrors();
		
	var f = document.emailSignupFooter;
	var hasError = false;
	var errorLog = [];
	
	// check date of birth
	if (!fc.isValidDob()) {
		jQuery("#footer-signup .dob-error").css("display", "block");
		hasError = true;
	}
	
	// UK postal code validation
    if (jQuery('#countryField').val()=='UK') {
        if (ess.validateUKPostcode(jQuery.trim(jQuery('input[name="zip"]').val())) == false) {
            errorLog.push({name: "zip", value: ess.locale['invalidZipCode']});
            hasError = true;
        }
    }
	
	if (!hasError) {
		
		var formData = {
			"emailAddress": f.emailAddress.value,
			"mobilephone": f.mobilephone.value,
			"zip": f.zip.value,
			"country": f.country.value,
			"contest": "RLM_FOOTER_FR",
			"email_pref": "Y"
		};
		
		if (f.birthDay.value != "0" && f.birthMonth.value != "0") {
			formData.birthDay = f.birthDay.value;
			formData.birthMonth = f.birthMonth.value;
		}
		
		if (f.emailUpdates.checked) {
			formData.emailUpdates = "on";
		}
		
		// submit the form!
		jQuery.ajax({
			type: "POST",
			url: "/footersignup/index.jsp",
			data: formData,
			success: function() {
				// hide form to show thank you message
				jQuery("#footer-signup-inner").css("visibility", "hidden");
			}
		});
	} else {
		// display errors
		jQuery("#fec-inner #email-error").css("display", "block");
		jQuery.each(errorLog, function() {
			fc.setError(this.name, this.value);
		});
		jQuery("html, body").animate({ scrollTop: 0 });
	}
}

fc.isValidDob = function() {
	var day = document.emailSignupFooter.birthDay.value;
	var month = document.emailSignupFooter.birthMonth.value;
	if (day == 0 && month == 0) {
		return true;
	} else {
		if (day != 0 && month != 0) {
			return true;
		} else {
			return false;
		}
	}
}

fc.setError = function(inputName, errorMsg) {
	jQuery("div.field [name='" + inputName + "']").addClass("fieldError")
		.next(".error").html(errorMsg).css("display", "block");
}

fc.clearErrors = function() {
	jQuery("#fec-inner #email-error").css("display", "none");
	jQuery("div.dob-error").css("display", "none");
	var allFields = ["emailAddress", "mobilephone", "zip", "country", "birthDay", "birthMonth"];
	jQuery.each(allFields, function(i) {
		jQuery("div.field [name='" + allFields[i] + "']").removeClass("fieldError")
			.next(".error").html("").css("display", "none");
	});
}


