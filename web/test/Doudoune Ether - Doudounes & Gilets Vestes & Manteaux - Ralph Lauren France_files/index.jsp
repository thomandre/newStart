function footerSignUp(email, mode) {
	// choose version
	var extended = (mode == 1) ? "block" : "none";
	jQuery("#footer-signup #extended").css("display", extended);
	// populate disabled email field
	document.emailSignupFooter.emailAddress.value = email;
